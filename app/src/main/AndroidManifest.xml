<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.play.demoplay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.OneActivity" />
        <activity android:name=".activity.AnimatorActivity"/>
        <activity android:name=".activity.VectorActivity"/>
        <activity android:name=".openGl.OpenGlActivity"/>
        <activity android:name=".activity.XmlAnimActivity"/>
        <activity android:name=".svg.SvgActivity"/>
        <activity android:name=".anim.AnimDownActivity"/>
    </application>

</manifest>